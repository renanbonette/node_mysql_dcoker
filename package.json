{
  "name": "user_microservice",
  "version": "1.0.0",
  "description": "Microserviço de usuários utilizando node, mysql, dockre, eslint, flow, testes unitários/integração/contrato",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=prod babel-node index.js",
    "start-dev": "NODE_ENV=dev nodemon --exec babel-node index.js",
    "test": "npm run lint && npm run test-unit && npm run test-integration && npm run test-contract",
    "test-integration": "NODE_ENV=test mocha --opts test/integration/mocha.opts test/integration/**/*.js",
    "test-unit": "NODE_ENV=test mocha --opts test/unit/mocha.opts test/unit/**/*.js",
    "test-contract": "NODE_ENV=test mocha --opts test/contracts/mocha.opts test/contracts/*.js",
    "lint": "eslint '**/*.js'",
    "lint:fix": "eslint '**/*.js' --fix",
    "flow": "flow",
    "flow-all": "flow check --all"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.16.0",
    "express": "^4.14.0",
    "http-status": "^1.0.0",
    "jwt-simple": "^0.5.1",
    "mysql": "^2.13.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "sequelize": "^3.30.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-flow": "^6.23.0",
    "chai": "^3.5.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "flow-bin": "^0.50.0",
    "joi": "^10.2.0",
    "joi-assert": "0.0.3",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "promise": "^8.0.1",
    "supertest": "^2.0.1",
    "testdouble": "^1.11.0",
    "webpack": "^1.14.0"
  }
}
